.placeholder-container {
  background-color: #f6f6f6;
  min-height: 100vh;
  padding: 20rpx;
}

.placeholder-header {
  height: 80rpx;
  background-color: #f0f0f0;
  margin-bottom: 20rpx;
  border-radius: 8rpx;
}

.placeholder-content {
  background-color: #ffffff;
  border-radius: 8rpx;
  padding: 10rpx;
}

.placeholder-item {
  height: 100rpx;
  background-color: #f5f5f5;
  margin: 20rpx 0;
  border-radius: 8rpx;
} 